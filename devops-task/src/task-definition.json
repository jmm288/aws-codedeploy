{
  "family": "php-app",
  "containerDefinitions": [
    {
      "name": "php-app",
      "image": "${REPOSITORY_URL}:latest",
      "cpu": 2,
      "memory": 300,
      "portMappings": [
          {
              "containerPort": 80,
              "hostPort": 8000
          }
      ],
      "essential": true,
      "environment": [
          {
              "value": "production"
          }
      ]
    }
 ]
}
